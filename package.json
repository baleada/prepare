    "@types/postcss-preset-env": "^6.7.3",
    "@types/puppeteer-core": "^5.4.0",
    "@types/refractor": "^3.0.2",
    "@types/tailwindcss": "^3.0.2",
    "@vitejs/plugin-react": "^1.1.4",
    "@vitejs/plugin-vue": "^2.0.1",
    "autoprefixer": "^10.4.2",
    "markdown-it": "^12.3.2",
    "markdown-it-link-attributes": "^4.0.0",
    "parse5": "^6.0.1",
